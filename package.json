{
  "name": "cnxt",
  "description": "Dynamic namespaces for dynamic content. #://HashtagProtocol",
  "version": "0.2.162",
  "author": "bresnow",
  "license": "MIT",
  "module": "es2022",
  "resolutions": {
    "react": "18.1.0-next-fc47cb1b6-20220404",
    "react-dom": "18.1.0-next-fc47cb1b6-20220404"
  },
  "scripts": {
    "build": "cross-env NODE_ENV=production remix build",
    "build:css": "unocss \"**/*.tsx\" --out-file \"app/uno.css\"",
    "build:worker": "esbuild app/entry.worker.tsx --outfile=public/entry.worker.js --minify --bundle --format=esm --define:process.env.NODE_ENV='\"production\"'",
    "build:start":"zx scripts/esm/build-start.mjs",
    "dev": "npx zx scripts/esm/dev.mjs",
    "postinstall": "patch-package && remix setup node",
    "prettier": "prettier --write '**/*.{json,md,mdx,mjs,js,jsx,ts,tsx}'",
    "purge": "rm -rf radata *.tmp **/*/! **/*/%1C",
    "push": "zx scripts/esm/git/push.mjs",
    "start": "cross-env-shell NODE_ENV=production PUB=$PUB PRIV=$PRIV EPUB=$EPUB EPRIV=$EPRIV DOMAIN=$DOMAIN PORT=$PORT PEER_DOMAIN=$PEER_DOMAIN node ./build/index.js"
  },
  "dependencies": {
    "@remix-run/node": "0.0.0-experimental-4e814511",
    "@remix-run/react": "0.0.0-experimental-4e814511",
    "@remix-run/server-runtime": "0.0.0-experimental-4e814511",
    "bresnow_utility-react-hooks": "0.2.9",
    "chainlocker": "^1.1.2",
    "chalk": "^5.0.1",
    "chokidar": "^3.5.3",
    "cross-env": "^7.0.3",
    "dockerode": "^3.3.1",
    "esbuild": "^0.14.47",
    "fast-glob": "^3.2.11",
    "fs-extra": "^10.1.0",
    "gun": "^0.2020.1237",
    "lz-object": "^0.1.7",
    "mime": "^3.0.0",
    "patch-package": "^6.4.7",
    "react": "18.0.0-rc.0",
    "react-dom": "18.0.0-rc.0",
    "redaxios": "^0.5.0",
    "remix": "0.0.0-experimental-4e814511",
    "zx": "^6.1.0"
  },
  "devDependencies": {
    "@remix-run/dev": "0.0.0-experimental-4e814511",
    "@remix-run/eslint-config": "^1.4.0",
    "@types/dockerode": "^3.3.9",
    "@types/gun": "^0.9.3",
    "@types/jsesc": "^3.0.1",
    "@types/lodash": "^4.14.181",
    "@types/lz-string": "^1.3.34",
    "@types/mime": "^2.0.3",
    "@types/node": "^17.0.35",
    "@types/object-assign": "^4.0.30",
    "@types/react": "^18.0.14",
    "@types/react-dom": "^18.0.5",
    "@types/uuid": "^8.3.4",
    "@unocss/cli": "^0.38.2",
    "@unocss/preset-attributify": "^0.38.2",
    "@unocss/preset-typography": "^0.38.2",
    "autoprefixer": "^10.4.4",
    "concurrently": "^7.3.0",
    "eslint": "^8.13.0",
    "nodemon": "^2.0.15",
    "postinstall": "^0.7.4",
    "prettier": "2.7.0",
    "typescript": "^4.7.3",
    "unocss": "^0.38.2"
  },
  "engines": {
    "node": ">=14"
  },
  "sideEffects": false
}
